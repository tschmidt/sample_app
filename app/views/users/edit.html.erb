<% provide(:title, "Edit user") %> 
<h1>Update your profile</h1>

<%= render layout: 'form' do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation, label: 'Confirm Password' %>
  <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<%- end -%>